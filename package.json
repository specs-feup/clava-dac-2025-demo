{
  "name": "@specs-feup/clava-fccm-2025-demo",
  "version": "1.0.0",
  "author": "Tiago Lascasas Santos <tiagolascasas@outlook.com> (https://tiagolascasas.dev)",
  "description": "Clava demo presented at the FCCM 2025 Demo Night",
  "type": "module",
  "files": [
    "api",
    "src",
    ".gitignore",
    "jest.config.js",
    "LICENSE",
    "package-lock.json",
    "package.json",
    "README.md",
    "tsconfig.jest.json",
    "tsconfig.json",
    "typedoc.config.js"
  ],
  "scripts": {
    "demo:s00": "npx clava classic dist/S00_PrintCall.js -p inputs/scenario0/BasicScenario.c -of outputs/s00/",
    "demo:s01": "npx clava classic dist/S01_PrintCallInteractive.js -p inputs/scenario0/BasicScenario.c -of outputs/s01/",
    "demo:s10": "npx clava classic dist/S10_StructFlattening.js -p inputs/scenario1/StructFlatteningScenario.c -of outputs/s10/",
    "demo:s20": "npx clava classic dist/S20_Synthesis.js -p inputs/scenario2/SynthesisScenario.c -of outputs/s20/",
    "demo:s30": "npx clava classic dist/S30_EtgTransforms.js -p inputs/scenario3/edge_detect.cpp -ncg",
    "demo:s31": "npx clava classic dist/S31_EtgFull.js -p inputs/scenario3/edge_detect.cpp -ncg",
    "demo:s32": "npx clava classic dist/S32_EtgMerging.js -p inputs/scenario3/edge_detect.cpp -ncg",
    "demo:s33": "npx clava classic dist/S33_EtgSplitting.js -p inputs/scenario3/edge_detect.cpp -ncg",
    "build": "tsc",
    "build:watch": "tsc --watch"
  },
  "dependencies": {
    "@specs-feup/clava": "^3.0.5",
    "@specs-feup/clava-code-transforms": "^1.0.0",
    "@specs-feup/clava-vitis-integration": "^1.0.0",
    "@specs-feup/extended-task-graph": "^0.9.1",
    "@specs-feup/lara": "^3.0.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@specs-feup/clava-visualization": "^1.0.5",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.2",
    "typedoc": "^0.26.4",
    "typescript": "^5.5.3"
  }
}
